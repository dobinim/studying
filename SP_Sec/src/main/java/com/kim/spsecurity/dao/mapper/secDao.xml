<?xml version="1.0" encoding="UTF-8"?>
<!-- mapper문서형식 지정 -->
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
				<!-- mapper의 namespace : 인터페이스의 경로 -->
<mapper namespace = "com.kim.spsecurity.dao.ISecDao">
	<insert id = "join" parameterType = "com.kim.spsecurity.dto.JoinDto">
		INSERT INTO USERDB(PID, PPW, PADDRESS, PHOBBY, PPROFILE) VALUES (#{pid}, #{ppw}, #{paddress}, #{phobby}, #{pprofile})
	</insert>
</mapper>